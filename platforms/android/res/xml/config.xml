<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Workout App</name>
    <description>
        This app will help you quickly put together a workout plan so that you can stop planning that shit, and just workout.
    </description>
    <author email="cstehm@comcast.net" href="http://www.chrisstehm.com">
        Chris Stehm
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="http://71.168.189.93/*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
